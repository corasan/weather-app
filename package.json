    "postinstall": "say -v 'Samantha' 'finished installing all dependencies'"
  },
  "dependencies": {
    "@react-native-community/async-storage": "^1.6.3",
    "@react-native-firebase/admob": "^6.0.4",
    "@react-native-firebase/app": "^6.0.4",
    "appcenter": "^2.6.0",
    "appcenter-analytics": "^2.6.0",
    "appcenter-crashes": "^2.6.0",
    "lodash": "^4.17.15",
    "moment": "^2.24.0",
    "react": "16.12.0",
    "react-native": "0.61.4",
    "react-native-code-push": "^5.7.0",
    "react-native-drawer": "^2.5.1",
    "react-native-gesture-handler": "^1.5.1",
    "react-native-location": "^2.5.0",
    "react-native-vector-icons": "^6.6.0",
    "react-navigation": "^4.0.10",
    "react-navigation-stack": "^1.10.3"
  },
  "devDependencies": {
    "@babel/core": "^7.7.2",
    "@babel/runtime": "^7.7.2",
