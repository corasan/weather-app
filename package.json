{
  "name": "cloudmate",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "start": "say -v 'Samantha' 'starting server' && react-native start",
    "test": "jest",
    "lint": "eslint .",
    "cp-staging": "say -v 'Samantha' 'codepushing to staging' && appcenter codepush release-react -a corasan/WeatherApp -s Staging",
